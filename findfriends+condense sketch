candidates = a,b,c,d

findFriend(tile, set)
	if tile == 0:
		return
	while set[next] == 0:
		next+=1
	if tile == set[next]:
		set[next]=0
		tile*=2
		return
	else:
		return

for i in range(3):
	findFriend(i)

condense(set):
	counter = 0
	for i in range(4):
		if set[i]!=0:
			set[counter] = set[i]
			counter++
		else:
			nothing happends